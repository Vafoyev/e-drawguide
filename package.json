{
  "name": "e-drawguide-backend",
  "version": "1.0.0",
  "description": "Student Education Platform (LMS) Backend",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@sentry/node": "^7.100.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.1.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "file-type": "^16.5.4",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.9.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "semver": "^7.7.3",
    "sequelize": "^6.37.3",
    "sharp": "^0.34.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.2.5",
    "supertest": "^7.2.2"
  },
  "overrides": {
    "tar": "7.5.7",
    "bcrypt": {
      "@mapbox/node-pre-gyp": {
        "tar": "7.5.7"
      }
    }
  }
}